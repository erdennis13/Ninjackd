<% if current_user.try(:admin?) %>

<div class="tabbable"> 
	<ul class="nav nav-pills">
	  <li class="active"><a href="#tab1" data-toggle="pill" data-no-turbolink="true" class="btn-xs">Subscription History</a></li>
	  <li><a href="#tab2" data-toggle="pill" data-no-turbolink="true" class="btn-xs">Weeklyplan Info</a></li>
	  <li><a href="#tab3" data-toggle="pill" data-no-turbolink="true" class="btn-xs">User Info</a></li>
	  <li><a href="#tab4" data-toggle="pill" data-no-turbolink="true" class="btn-xs">Add New User</a></li>
	  <li><a href="#tab5" data-toggle="pill" data-no-turbolink="true" class="btn-xs">NinjaTips</a></li>
	  <li><a href="#tab6" data-toggle="pill" data-no-turbolink="true" class="btn-xs">Add NinjaTip</a></li>
	  <li><a href="#tab7" data-toggle="pill" data-no-turbolink="true" class="btn-xs">Reviews</a></li>
	</ul>

	<div class="tab-content">
		<div class="tab-pane active" id="tab1">
			<br/>
			<%= render 'adminSubscriptionCount' %>
		</div>
		<div class="tab-pane" id="tab3" >
			<br/>
			<%= render 'userCount' %>
		</div>
		<div class="tab-pane" id="tab2" >
			<br/>
			<%= render 'weeklyplanDetails' %>
		</div>
		<div class="tab-pane" id="tab4" >
			<br/>
			<%= render 'addnewuser' %>
		</div>
		<div class="tab-pane" id="tab5" >
			<br/>
			<%= render 'allNinjaTips' %>
		</div>
		<div class="tab-pane" id="tab6" >
			<br/>
			<%= render 'addNinjaTip' %>
		</div>
		<div class="tab-pane" id="tab7" >
			<br/>
			<%= render 'adminviewreviews' %>
		</div>
	</div>
</div>

<% else %>
	<%= render 'pages/unauth' %>
<% end %>

