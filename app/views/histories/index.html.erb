<div class="panel panel-default">
  <div class="panel-body">
    <h1>Listing histories</h1>

    <table class="table table-striped">
      <thead>
        <tr>
          <th>Subscription</th>
          <th>Exercise</th>
          <th>Complete</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @histories.each do |history| %>
          <tr>
            <td><%= history.subscription_id %></td>
            <td><%= history.exercise_id %></td>
            <td><%= history.complete %></td>
            <td><%= link_to 'Show', history %></td>
            <td><%= link_to 'Edit', edit_history_path(history) %></td>
            <td><%= link_to 'Destroy', history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

  </div>
  <div class="panel-footer">
    <%= link_to 'New History', new_history_path %>
  </div>
</div>