<% if current_user.try(:admin) %>
	<div class="panel panel-default">
		<div class="panel-body">
			<h1>Editing userreview</h1>

			<%= render 'form' %>
		</div>
		<div class="panel-footer">
			<%= link_to 'Show', @userreview %> |
			<%= link_to 'Back', userreviews_path %>
		</div>
	</div>
<% else %>
	<%= render 'pages/unauth' %>
<% end %>