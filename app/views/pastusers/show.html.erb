<% if current_user.try(:admin) %>
	<div class="panel panel-default">
		<div class="panel-body">
			<p>
			  <strong>Username:</strong>
			  <%= @pastuser.username %>
			</p>

			<p>
			  <strong>Email:</strong>
			  <%= @pastuser.email %>
			</p>
		</div>
		<div class="panel-footer">

			<%= link_to 'Edit', edit_pastuser_path(@pastuser) %> |
			<%= link_to 'Back', pastusers_path %>
		</div>
	</div>
<% else %>
	<%= render 'pages/unauth' %>
<% end %>