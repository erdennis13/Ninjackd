<% if current_user.try(:admin?) %>
	<p id="notice"><%= notice %></p>

	<p>
	  <strong>User:</strong>
	  <%= @like.user_id %>
	</p>

	<p>
	  <strong>Workout:</strong>
	  <%= @like.workout_id %>
	</p>

	<%= link_to 'Edit', edit_like_path(@like) %> |
	<%= link_to 'Back', likes_path %>
<% else %>
	<%= render 'pages/unauth' %>
<% end %>